ren, no need to proceed.\n        if (!node || !node.children) return;\n\n        // Convert HTMLCollection/NodeList to an array so we can safely iterate\n        // even if the children change during the process.\n        const children = Array.from(node.children);\n\n        for (const child of children) {\n            // Recursively handle child's subtree first.\n            stripTagsWithClass(child, className);\n\n            // After recursion, check if this child has the target class.\n            if (child.classList && child.classList.contains(className)) {\n            // Move all child nodes (inner text, nested elements, etc.)\n            // out of the element being removed.\n            while (child.firstChild) {\n                child.parentNode.insertBefore(child.firstChild, child);\n            }\n            // Remove the now-empty element from its parent.\n            child.parentNode.removeChild(child);\n            }\n        }\n      }\n\n      $(document).ready(function(){\n\n        /* default settings */\n        $('.venobox').venobox();\n\n\n        /* custom settings */\n        $('.venobox_custom').venobox({\n            framewidth: '400px',        // default: ''\n            frameheight: '300px',       // default: ''\n            border: '10px',             // default: '0'\n            bgcolor: '#5dff5e',         // default: '#fff'\n            titleattr: 'data-title',    // default: 'title'\n            numeratio: true,            // default: false\n            infinigall: true            // default: false\n        });\n\n        /* auto-open #firstlink on page load */\n        $(\"#firstlink\").venobox().trigger('click');\n\n        const $whatson = $('#whats-on')[0];\n\n        $('#whats-on .day').click(function () {\n          var $this = $(this);\n          if (!$this.hasClass('select')) {\n            var index = $this.data('day-index');\n            $('.day').removeClass('select');\n            $this.addClass('select');\n\n          